{
  "name": "ai-browser-researcher-integrated",
  "private": true,
  "version": "2.1.0",
  "type": "module",
  "packageManager": "pnpm@9.7.0",
  "bin": {
    "ai-research": "./dist/cli.js"
  },
  "scripts": {
    "build": "pnpm -r build && tsup src/cli.ts --format esm --outDir dist",
    "build:honest": "tsup src/cli-honest.ts --format esm --outDir dist --outExtension:.js=.honest.js",
    "dev": "pnpm -r dev",
    "start:live": "pnpm --filter live-dashboard start",
    "type-check": "pnpm -r exec tsc --noEmit",
    "lint": "eslint .",
    "research": "tsx src/cli.ts research",
    "research:honest": "tsx src/cli-honest.ts research",
    "research:compare": "tsx src/cli-honest.ts compare",
    "research:metrics": "tsx src/cli-honest.ts metrics",
    "dashboard": "tsx src/cli.ts dashboard",
    "memorize": "tsx src/cli.ts memorize",
    "remember": "tsx src/cli.ts remember"
  },
  "dependencies": {
    "commander": "^12.1.0"
  },
  "devDependencies": {
    "eslint": "^9.8.0",
    "typescript": "^5.5.4",
    "tsup": "^8.1.0",
    "tsx": "^4.15.7",
    "zod": "^3.23.8"
  }
}